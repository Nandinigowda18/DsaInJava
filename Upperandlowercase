import java.util.*;
public class UPperandlowercase {
	
	public static String prob(String str) {
		int capCount=0;int lowcount=0;String ans;
		for(int i=0;i<str.length();i++) {
			if(str.charAt(i)>=65 & str.charAt(i)<97) {
				capCount++;
			}else {
				lowcount++;
			}
		}
		if(capCount >lowcount) {
		 
		 ans = str.toUpperCase();
		}else {
			ans = str.toLowerCase();
		}
	
	return ans.toString();
	}
	

	public static void main(String[] args) {
		// TODO Auto-generated method stub
System.out.println(prob("abCFRggJJJa"));
	}

}
